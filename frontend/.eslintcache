istGroup>\n        )}\n      </Col>\n      <Col md={4}>\n        <Card>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h2>\n                Subtotal ({cartItems.reduce((acc, item) => acc + item.qty, 0)})\n                items{' '}\n              </h2>\n              $\n              {cartItems\n                .reduce((acc, item) => acc + item.qty * item.price, 0)\n                .toFixed(2)}\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <Button\n                type='button'\n                className='btn-block'\n                disabled={cartItems.length === 0}\n                onClick={checkoutHandler}\n              >\n                Proceed to Checkout\n              </Button>\n            </ListGroup.Item>\n          </ListGroup>\n        </Card>\n      </Col>\n    </Row>\n  );\n};\n\nexport default CartScreen;\n","/home/nick/Documents/serafini_stitches/frontend/src/reducers/cartReducers.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/constants/cartConstants.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/actions/cartActions.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/reducers/userReducers.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/constants/userConstants.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/screens/LoginScreen.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/components/FormContainer.js",[],"/home/nick/Documents/serafini_stitches/frontend/src/actions/userActions.js",["111"],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":9,"column":8,"nodeType":"118","messageId":"119","endLine":9,"endColumn":19},{"ruleId":"116","severity":1,"message":"120","line":2,"column":3,"nodeType":"118","messageId":"119","endLine":2,"endColumn":23},{"ruleId":"116","severity":1,"message":"121","line":3,"column":3,"nodeType":"118","messageId":"119","endLi